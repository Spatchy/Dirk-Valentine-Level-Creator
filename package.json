{
  "name": "dirk-valentine-level-creator",
  "version": "0.1.0",
  "private": true,
  "author": {"name": "Spatchy"},
  "description": "An app for creating and editing Dirk Valentine levels",
  "type": "module",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve --experimental-modules",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "main": "src/main/background.js",
  "dependencies": {
    "bulma": "^0.9.3",
    "core-js": "^3.6.5",
    "electron-squirrel-startup": "^1.0.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.7.1",
    "merge-images-v2": "^2.0.1",
    "node-pty": "^0.10.1",
    "replace-color": "^2.3.0",
    "sharp": "^0.29.3",
    "sudo-prompt": "^9.2.1",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@vue/cli-plugin-babel": "~5.0.0-rc.0",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.0",
    "@vue/cli-service": "~5.0.0-rc.0",
    "@vue/compiler-sfc": "^3.0.0",
    "css-loader": "^6.5.1",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-rebuild": "^3.2.3",
    "eslint-plugin-vue": "^7.0.0",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.3.0",
    "vue-cli-plugin-electron-builder": "^2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "dirk_valentine_level_creator"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
